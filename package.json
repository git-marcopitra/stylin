{
  "name": "stylin.js",
  "version": "0.4.1-beta",
  "description": "A react component inline styles statement lib",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --clearCache && jest --collectCoverage",
    "prepare": "husky install",
    "publish": "npm publish",
    "prepublish": "yarn build",
    "format": "prettier --write **/*.ts",
    "lint": "eslint . --fix --ext ts",
    "storybook": "start-storybook -p 6006",
    "build": "rimraf dist && tsc",
    "build:storybook": "rimraf storybook-static && build-storybook"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/git-marcopitra/stylin.git"
  },
  "keywords": [
    "style",
    "react",
    "builtin",
    "inline-styles",
    "react-component",
    "stylin-component",
    "design-system"
  ],
  "author": "git-marcopitra@github.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/git-marcopitra/stylin.js/issues"
  },
  "homepage": "https://git-marcopitra.github.io/stylin.js/",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/eslint-plugin": "^11.10.0",
    "@react-theming/storybook-addon": "^1.1.10",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "babel-jest": "^29.4.2",
    "babel-loader": "^8.3.0",
    "commitlint": "^17.4.2",
    "commitlint-config-gitmoji": "^2.2.10",
    "csstype": "^3.1.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.0",
    "jest": "^29.4.2",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5"
  }
}
